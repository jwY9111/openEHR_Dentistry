definition
	CLUSTER[at0000.1] matches {    -- Periodontal charting
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001.1] matches {    -- System or structure examined
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::at0.73]    -- Gingiva (Gum)
						}
					}
				}
			}
			ELEMENT[at0012.1] occurrences matches {0..1} matches {    -- Tooth position
				value matches {
					DV_CODED_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0011.1] occurrences matches {0..*} matches {    -- Structured tooth position
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_circle(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_relative\.v2/}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {    -- Examination findings
				include
					archetype_id/value matches {/.*/}
			}
			allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Multimedia representation
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Clinical description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0006] occurrences matches {0..*} matches {    -- Clinical interpretation
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {    -- Examination not done
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0.6] occurrences matches {0..1} matches {    -- Condition
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.7,    -- Unrestored or Sound
							at0.8,    -- Restored or Filled
							at0.53,    -- Carious or Decayed
							at0.54,    -- Recurrent or Secondary Caries
							at0.55,    -- Compromised Restoration
							at0.56,    -- Tooth Wear
							at0.57,    -- Fracture
							at0.58,    -- Discoloration
							at0.59,    -- Tooth Anomaly - Postion
							at0.60,    -- Tooth Anomaly - Size
							at0.61,    -- Tooth Anomaly - Shape
							at0.62,    -- Tooth Anomaly - Number
							at0.63]    -- Tooth Anomaly - Structure
						}
					}
				}
			}
			ELEMENT[at0.3] occurrences matches {0..1} matches {    -- Presence
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.4,    -- Present
							at0.5]    -- Absent
						}
					}
				}
			}
			ELEMENT[at0.9] occurrences matches {0..1} matches {    -- General description of gingiva
				value matches {
					DV_TEXT matches {*}
				}
			}
			CLUSTER[at0.11] occurrences matches {0..*} matches {    -- Findings
				items cardinality matches {1..*; unordered} matches {
					CLUSTER[at0.210] occurrences matches {0..1} matches {    -- Charting
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0.211] occurrences matches {0..1} matches {    -- Examined tooth name
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0.38] occurrences matches {0..1} matches {    -- Notation system
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0.39,    -- FDI/ ISO notation system
											at0.40,    -- Zsigmondy-Palmer notation system
											at0.41]    -- Universal/ ADA notation system
										}
									}
								}
							}
							ELEMENT[at0.2] occurrences matches {0..1} matches {    -- Tooth label (Tooth number)
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0.64] occurrences matches {0..1} matches {    -- Mobility
								value matches {
									0|[local::at0.65],
									1|[local::at0.66],
									2|[local::at0.67],
									3|[local::at0.68]
								}
							}
							CLUSTER[at0.186] occurrences matches {0..*} matches {    -- Pocket depth
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0.10] occurrences matches {0..1} matches {    -- Pocket depth measurement
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::122]>
												list = <
													["1"] = <
														units = <"mm">
														magnitude = <|0.0..20.0|>
														precision = <|0|>
													>
												>
											>
										}
									}
									ELEMENT[at0.187] occurrences matches {0..1} matches {    -- Surface
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0.188,    -- Labial
													at0.189,    -- Buccal
													at0.190,    -- Mesial
													at0.191,    -- Distal
													at0.192,    -- Palatal
													at0.193]    -- Lingual
												}
											}
										}
									}
								}
							}
							ELEMENT[at0.93] occurrences matches {0..1} matches {    -- Suppuration
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
							CLUSTER[at0.169] occurrences matches {0..1} matches {    -- Gingival recession
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0.194] occurrences matches {0..1} matches {    -- Surface
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0.195,    -- Labial
													at0.196,    -- Buccal
													at0.197,    -- Mesial
													at0.198,    -- Distal
													at0.199,    -- Palatal
													at0.200]    -- Lingual
												}
											}
										}
									}
									ELEMENT[at0.170] occurrences matches {0..1} matches {    -- Type
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0.171,    -- Localized gingival recession
													at0.172]    -- Generalized gingival recession
												}
											}
										}
									}
									ELEMENT[at0.95] occurrences matches {0..1} matches {    -- Classification (Miller classification)
										value matches {
											1|[local::at0.206],
											2|[local::at0.207],
											3|[local::at0.208],
											4|[local::at0.209]
										}
									}
									ELEMENT[at0.173] occurrences matches {0..1} matches {    -- Description
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
							ELEMENT[at0.201] occurrences matches {0..1} matches {    -- Furcation involvement
								value matches {
									1|[local::at0.202],
									2|[local::at0.203],
									3|[local::at0.204],
									4|[local::at0.205]
								}
							}
						}
					}
					ELEMENT[at0.120] occurrences matches {0..1} matches {    -- Bleeding on probing
						value matches {
